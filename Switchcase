
        public class Program {
    public static void main(String[] args) {
         int day=3;
switch(day){
    case 1:
    System.out.println("friday");
    break; 
    case 2:
    System.out.println("saturday");
    break;
    case 3:
    System.out.println("sunday");
    break;
    default :
    System.out.println("there was no match!");
}
}
}
